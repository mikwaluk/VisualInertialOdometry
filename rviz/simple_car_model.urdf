<?xml version='1.0'?>
<!-- Borrowed from https://answers.ros.org/question/281717/urdf-rviz-robotmodel-not-showing-correctly/-->
<robot name="fourwheelrobot">
  <!-- World frame -->
  <link name="world" />
  <joint name="joint_base" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0" /> 
  </joint>
  <!-- Base link -->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size="3 1.5 0.75"/>
      </geometry>
      <material name="yellow">
        <color rgba="0.8 0.8 0 1"/>
      </material>
    </visual>
  </link>
  <!-- Glass in the upper part -->
  <link name="glass_upper_part">
    <visual>
      <origin xyz="-0.9 0 0" />
      <geometry>
          <box size="1.8 1.5 1.2"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 0.8 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_upper_part" type="fixed">
    <parent link="base_link"/>
    <child link="glass_upper_part"/>
    <origin xyz="0.3 0 0.975" rpy="0 0 0" /> 
  </joint>
  <!-- imu_base - -->
  <link name="imu0">
    <visual>
      <origin xyz="0 0 0" />
      <geometry>
          <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>
  <link name="cam0">
    <visual>
      <origin xyz="0 0 0" />
      <geometry>
          <box size="0.02 0.02 0.02"/>
      </geometry>
    </visual>
  </link>
  <joint name="joint_imu" type="fixed">
    <parent link="glass_upper_part"/>
    <child link="imu0"/>
    <origin xyz="0 0 0" rpy="1.570795 0 1.570795" /> 
  </joint>
  <joint name="joint_camera" type="fixed">
    <parent link="imu0"/>
    <child link="cam0"/>
    <origin xyz="0.039 -0.006 0" rpy="0 0 0" /> 
  </joint>
  <!-- Front Right Wheel -->
  <link name="f_r_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.3" radius="0.6" />
      </geometry>
      <material name="black">
        <color rgba="0.05 0.05 0.05 1"/>
      </material>
    </visual>
  </link>
  <joint name="joint_f_r_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="f_r_wheel"/>
    <origin xyz="0.75 -0.90 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>  
  <!-- Back Right Wheel -->
  <link name="b_r_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.3" radius="0.6" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_b_r_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="b_r_wheel"/>
    <origin xyz="-0.75 -0.90 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>  
  <!-- Front Left Wheel -->
  <link name="f_l_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.3" radius="0.6" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_f_l_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="f_l_wheel"/>
    <origin xyz="0.75 0.90 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
  <!-- Back Left Wheel -->
  <link name="b_l_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
          <cylinder length="0.3" radius="0.6" />
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="joint_b_l_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="b_l_wheel"/>
    <origin xyz="-0.75 0.90 0" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
</robot>
